MODULE user_command_0002 INPUT.

  CASE sy-ucomm.

    WHEN 'BACK1'.

      LEAVE TO SCREEN '0001'.

    WHEN 'SAIR' OR 'VOLTAR' OR 'CANCELAR'.

      LEAVE TO SCREEN 0.

  ENDCASE.

ENDMODULE.
